/*@import "ff_mod.css";*/
.index{
	background:#edebec;
}
scroll-view view{
  	width:auto;
}
/*nav*/
.ff_nav{
	position: relative;
	background:#fff;
}
.line_nav{
	height:30px;line-height:30px;
	padding:3px 0;
	display:-webkit-flex;
    display:flex;
    overflow:hidden;
	width:98%;  
}
.line_nav li{
	-webkit-flex:initial;
    flex:initial;
    padding:0 10px;
    text-align:center;
}
.line_nav a{
	font-size:14px; color:#666;	
}
.line_nav .current{
	color:#4b943c;
	border-bottom:2px solid #4b943c;
}
.open_bg{
	background:#000;opacity:0.5;position:fixed;left:0;top:0;width:100%;height:100%;display:none;z-index:1; 
}
.open_h{font-size:0.24rem;color:#333;display:none;width:100%; background:#f8f6f7;
	height:0.8rem;line-height:0.8rem;padding-left:0.3rem;position:absolute;left:0;top:0;z-index:10; 
}
.img_box{
	position:absolute;right:0;top:0.1rem;display:block;
	width:0.9rem;height:0.52rem;padding-top:0.08rem;text-align:center;
	background:#fff;border-left:0.02rem solid #e5e5e5;z-index:10; 
}
.img_box img{width:0.26rem;height:0.16rem;
	-webkit-transition: all 0.4s ease;
	-o-transition: all 0.4s ease;
	transition: all 0.4s ease;
}
.img_box_on{border-left:none;background:#f8f6f7;}
.img_box_on img{
    -webkit-transform: rotate(180deg);
	-ms-transform: rotate(180deg);
	-o-transform: rotate(180deg);
	transform: rotate(180deg);
}
/*open_all_nav*/
.open_munu{
	background:#fff;
	border-collapse:collapse;
	width:100%;display:none;
	position:absolute;left:0;top:0.8rem;z-index:10;
}
.open_munu td{
	vertical-align:middle;text-align:center;
	width:25%;
	height:1.6rem;
	border:2px solid #e5e5e5;
}
.open_munu td a{
	display:inline-block;min-width:0.58rem;
	font-size:0.2rem;color:#666;
}
.open_munu img{	
	width:0.58rem;height:0.58rem;
}
.open_munu .cur img{
	background:#ffee3c;
}
/*banner*/
swiper{height:200px}
swiper image{width:100%;display:block;}

.banner {
	width: 100%;
	position: relative;
}
.banner .swipe {
	overflow: hidden;
	position: relative;
}

/*topic*/
.scroll-view_b{margin-top:10px;}
.scroll-view-item_b{width:24%;height:100%;margin:0px 5px;}
.scroll-view-item_b image{height:auto;width:100%;display:inline;}
.nav_b{background: #f8f6f7;padding-bottom:15px;padding-top:6px;}
/*report*/
.report {background: #f8f6f7;overflow:hidden;border-top:1px solid #dedcdd;padding:4px 10px;}
.report title{display:none;}
.report swiper{height:25px;width:70%;float:left;margin-left:10px;}
.report swiper text{line-height:30px;font-size:13px;color:#333;white-space:nowrap;}
.report image{width:20%;float:left;height:18px;margin-top:6px;}
/*commend*/
.recommend{margin-top:10px;background-color:#f8f6f7;}
.commend{
	width:100%;
	display:inline-block;

	background:#f8f6f7;
	margin-top:10px;
	border-bottom:0.02rem solid #dedcdd;
	color:#333;
	font-size:15px;
	height:40px;
	line-height:40px;
	text-align:center;
	font-weight:600;

}
.commend_list{
	background:#f8f6f7;

}
.commend_list li{
	width:100%;
	display:block;
	height:135px;
  	overflow:hidden;
  	padding-left:0.25rem; 
  	position:relative;
  	border-bottom:1px solid #dedcdd;
}
.commend_list .image{
	display:inline-block;
	width:30%;
	margin-left:3%;
}
.commend_list li image{
	width:100%;
	height:135px;
	float:left;
}
.des{
	float:right;
	text-align:right;
	padding:0.2rem 0 0 0;
	line-height:150%;
	width:60%;
	margin-right:5%;

}
.des view{
	line-height:25px;
}
.des_title,.des_info{
	font-size:12px;color:#666;
	overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.des_info{
	font-size:14px;font-weight:600;color:#333;
}
.des_old_m{
	font-size:0.18rem;color:#999;
	margin-right:0.1rem; 
}
.des_now_m{
	background:url(../images/$.jpg) no-repeat 0.05rem 0.18rem;padding-left:0.22rem;
	background-size:0.15rem 0.18rem;
	color:#ff5656;
	font-size:15px;font-weight:600;
}
.des_now_m i{}
.add_cart {
    padding:0 12px;
	height:30px;
	line-height:30px !important;
	display:inline-block;
	background:#ffee3c;
	border-radius:10px;
	text-align:center;
	font-size:13px;
	margin-top:0;

}
.more_sub_con{
	height:0.8rem;
	padding-top:0.2rem; 
	background:#f8f6f7;
	text-align:center;
}
.more_sub{
	display:inline-block;
	width:3.4rem;height:0.6rem;
	text-align:center;
	border:1px solid #4b943d;color:#4b943d;
	background:url(../images/more_sub.jpg) no-repeat 2.7rem center;
	background-size: 0.25rem 0.25rem; 
	border-radius:0.3rem;
	font-size:0.3rem;line-height: 0.58rem; 
}
.pro_mod_title{
	background:#f8f6f7;
	margin-top:0.2rem; 
	height:1rem;padding-top:0.25rem;
	text-align:center;
	color:#999;font-size: 0.24rem;line-height:150%;
}
.pro_mod_title h3{
	color:#333;font-size:0.26rem;font-weight:600;
	display:inline-block;text-align:left;
	padding-right: 0.2rem;
	background:url(..//images/h3_bg.jpg) no-repeat right center;
	background-size:0.12rem 0.2rem;
}
.pro_mod_list,.popular_pro{
	background:#f8f6f7;
	padding:0.2rem 0;
}
.pro_mod_list ul,.popular_pro ul{
	width:96%;margin:0 auto;
    overflow:hidden;
}
.pro_mod_list li{
	width:33%;
	float:left;
	text-align:center;
	position:relative;
}
.pro_mod_list li a{
	width:2rem;
	color:#666;
	font-size:0.2rem;line-height:130%;
	text-align:center;
}
.pro_mod_list li img{
	width:2.2rem;height:2.2rem;
	margin-bottom:0.08rem;
}
.mod_title{
	overflow:hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	line-height:140%;
	padding:0 0.1rem;
}
.mod_price{
	color:#ff5454;
	font-weight:600;
	margin-top:0.05rem;
}
/*popular*/
.popular_pro li{
	width:50%;text-align:center;
	float:left; 
	margin-bottom:0.2rem; 
	position:relative;
}
.popular_pro li img{
	width:3.4rem;
	height:3.4rem;
}
.popular_pro li a{
	font-size:0.24rem;color:#333;
	font-weight:600;line-height:180%;
	text-align:center;     
}
.popular_pro .title{
	overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.red_text{
	color:#ff5656;font-size:0.26rem;
	line-height:160%; 
}
.grey_text{
	color:#999;font-size:0.24rem;
	margin-left:0.1rem;line-height:160%; 
	font-weight:normal;
}
/*sold_out*/
.sold_out{
	color:#4b943c !important;
	background:#edebec;
}
/*category*/
.category_con{background:#f8f6f7;padding-top:0.2rem;text-align:left;font-family: "Helvetica Neue", Helvetica, sans-serif; }
.category_t{color:#333;
    font-size:0.28rem;font-weight: 600;
    height:0.5rem;line-height:0.5rem;text-align:center;
    }
.category_t span{background:url(../images/cate_icon.png) no-repeat right center;background-size:0.3rem 0.3rem;padding-right:0.4rem;}
.category_ul{padding:0.3rem 0 0.1rem;width:94%;margin:0 auto;text-align:center;overflow:hidden;}
.category_ul li{width:25%;float:left;text-align:center;padding-bottom:0.2rem;}
/*.category_ul td{padding-bottom:0.2rem;}*/
.category_ul img{width:1.4rem;height:1.4rem;margin-bottom:0.1rem;}
.category_ul span{display:block;font-size:0.26rem;color:#666;height:0.4rem;line-height:0.4rem;}
.c_mt20{margin-top:0.2rem;}